  <script>
    function ValueSetters() {
        gsap.set("#nav a", { y: "-100%", opacity: 0 })
        gsap.set("#home .parent .child", { y: "100%" })
        gsap.set("#home .row img", { opacity: 0 })

        document.querySelectorAll("#Visual>g").forEach(function (e) {
          var character = e.childNodes[1].childNodes[1]
          character.style.strokeDasharray = character.getTotalLength() + 'px';
          character.style.strokeDashoffset = character.getTotalLength() + 'px';
        })
      }
      function revealtoSpan() {
        document.querySelectorAll(".reveal")
          .forEach(function (elem) {
            let spanParent = document.createElement("span");
            let spanChild = document.createElement("span");

            spanParent.classList.add("parent");
            spanChild.classList.add("child");

            spanChild.innerHTML = elem.innerHTML;
            spanParent.appendChild(spanChild);

            elem.innerHTML = "";
            elem.appendChild(spanParent);

          })
      }
      function loaderAnimation() {
        var tl = gsap.timeline()

        tl
          .from("#loader .child span", {
            x: 100,
            stagger: .2,
            duration: 1.4,
            ease: Power3.easeInOut
          })
          .to("#loader .parent .child", {
            y: "-100%",
            duration: 1,
            ease: Circ.easeInOut
          })
          .to("#loader", {
            height: 0,
            duration: 1,
            ease: Circ.easeInOut
          })
          .to("#blue", {
            height: "100%",
            top: 0,
            duration: 1,
            delay: -1,
            ease: Circ.easeInOut
          })
          .to("#blue", {
            height: "0",
            duration: 1,
            delay: -.5,
            ease: Circ.easeInOut,
            onComplete: function () {
              animateHomepage();
            }
          })

      }
      function animate() {
        var typed = new Typed("#multiple", {
          strings: ["Designer", "Artist", "Designer"],
          typeSpeed: 100,
          backSpeed: 100,
          backDelay: 1500,
          loop: true,
          showCursor: false
        })

      }
      function animateSvg() {

        gsap.to("#Visual>g>g>path, #Visual>g>g>polyline", {
          strokeDashoffset: 0,
          duration: 2,
          ease: Expo.easeInOut
        })
      }
      function animateHomepage() {

        var tl = gsap.timeline();
        tl
          .to("#nav a", {
            y: 0,
            opacity: 1,
            stagger: 0.05,
            ease: Expo.easeInOut
          })

          .to("#home .parent .child", {
            y: 0,
            stagger: .1,
            duration: 2,
            ease: Expo.easeInOut
          })
          .to("#home .row", {
            opacity: 1,
            delay: -1,
            ease: Expo.easeInOut,
            onComplete: function () {
              animateSvg();
              animate();
            }
          })
          .to("#home #photo", {
            marginTop: "-2vh",
            opacity: 1,
            delay: -0.4,
            ease: Expo.easeInOut,
            duration: 3
          })
      }

      function locoIntialize() {
        const scroll = new LocomotiveScroll({
          el: document.querySelector('#main'),
          smooth: true
        });
      }
      function cardShow() {
        document.querySelectorAll(".cnt").forEach(function (cnt) {
          var showingImage;
          cnt.addEventListener("mousemove", function (dets) {
            document.querySelector("#cursor").children[dets.target.dataset.index].style.opacity = 1;
            showingImage = dets.target;
            console.log(dets)
            document.querySelector("#cursor").children[dets.target.dataset.index].style.transform = `translate(${dets.clientX}px, ${dets.clientY}px)`;
            showingImage.style.filter = "grayscale(1)"

            document.querySelector(".fullp").style.backgroundColor = "#" + dets.target.dataset.color;
            document.querySelector(".fp").style.backgroundColor = "#" + dets.target.dataset.color;
          })
          cnt.addEventListener("mouseleave", function (dets) {
            document.querySelector("#cursor").children[showingImage.dataset.index].style.opacity = 0;
            showingImage.style.filter = "grayscale(0)"
            document.querySelector(".fullp").style.backgroundColor = "#f2f2f2";
            document.querySelector(".fp").style.backgroundColor = "#f2f2f2";
          })
        })
      }
      function hover() {
        document.querySelector("#social").addEventListener("mouseenter", function () {
          document.querySelector("#social").style.backgroundColor = "#fff"
          document.querySelector("#sclink").style.display = "flex"
        })
        document.querySelector("#social").addEventListener("mouseleave", function () {
          document.querySelector("#social").style.backgroundColor = "transparent"
          document.querySelector("#sclink").style.display = "none"
        })
        document.querySelector("#categories").addEventListener("mouseenter", function () {
          document.querySelector("#categories").style.backgroundColor = "#fff"
          document.querySelector("#pagelink").style.display = "flex"
        })
        document.querySelector("#categories").addEventListener("mouseleave", function () {
          document.querySelector("#categories").style.backgroundColor = "transparent"
          document.querySelector("#pagelink").style.display = "none"
        })
      }
      function scrollt() {
        gsap.registerPlugin(ScrollTrigger);

        const locoScroll = new LocomotiveScroll({
          el: document.querySelector("#main"),
          smooth: true,

          tablet: { smooth: true },

          smartphone: { smooth: true }
        });
        locoScroll.on("scroll", ScrollTrigger.update);

        ScrollTrigger.scrollerProxy("#main", {
          scrollTop(value) {
            return arguments.length
              ? locoScroll.scrollTo(value, 0, 0)
              : locoScroll.scroll.instance.scroll.y;
          },
          getBoundingClientRect() {
            return {
              top: 0,
              left: 0,
              width: window.innerWidth,
              height: window.innerHeight
            };
          }

          // follwoing line is not required to work pinning on touch screen

          /* pinType: document.querySelector("#main").style.transform
            ? "transform"
            : "fixed"*/
        });

        ScrollTrigger.addEventListener("refresh", () => locoScroll.update());

        ScrollTrigger.refresh();
      }

      revealtoSpan();
      ValueSetters();
      loaderAnimation();
      locoIntialize();
      cardShow()
      hover();
      scrollt();
    

  </script>

